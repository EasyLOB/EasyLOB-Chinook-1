@model GenreCollectionModel

<div id="Ajax">
</div>

<script>
    $(function () {
        try {
            $("#Ajax").load("@(Url.Action("Search", "Genre", null, Request.Url.Scheme))");
        } catch (exception) {
            zExceptionMessage("Genre Index.cshtml", "function", exception.message)
        }
    });
</script>