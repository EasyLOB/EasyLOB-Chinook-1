@model InvoiceCollectionModel

<div id="Ajax">
</div>

<script>
    $(function () {
        try {
            $("#Ajax").load("@(Url.Action("Search", "Invoice", null, Request.Url.Scheme))");
        } catch (exception) {
            zExceptionMessage("Invoice Index.cshtml", "function()", exception.message)
        }
    });
</script>