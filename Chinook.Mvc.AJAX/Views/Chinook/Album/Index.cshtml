@model AlbumCollectionModel

<div id="Ajax">
</div>

<script>
    $(function () {
        try {
            $("#Ajax").load("@(Url.Action("Search", "Album", null, Request.Url.Scheme))");
        } catch (exception) {
            zExceptionMessage("Album Index.cshtml", "function()", exception.message)
        }
    });
</script>